{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "isolatedModules": true,
    "jsx": "react",
    "resolveJsonModule": false,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*",
        "./dist/*"
      ],
      "public/*": [
        "public/*"
      ],
      "assets/*": [
        "src/assets/*"
      ],
      "components/*": [
        "src/components/*"
      ],
      "contexts/*": [
        "src/contexts/*"
      ],
      "layouts/*": [
        "src/layouts/*"
      ],
      "libs/*": [
        "src/libs/*"
      ],
      "pages/*": [
        "src/pages/*"
      ],
      "routes/*": [
        "src/routes/*"
      ],
      "styles/*": [
        "src/styles/*"
      ],
      "types/*": [
        "src/@types/*"
      ],
      "utils/*": [
        "src/utils/*"
      ]
    }
  },
  "include": [
    "src",
    "vite.config.js"
  ]
}